<template name="location">
    <view class="delivery-info bg-w" bindtap="reLocation" data-address="{{item.address}}" data-location="{{item.location}}" data-title="{{item.title}}">  
      <view class="info-user">
        {{item.title}}
      </view>
      <view class="info-address">{{item.address}}</view>
    </view>
</template>   